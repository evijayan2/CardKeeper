CREATE TABLE insurance_cards (
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    providerName TEXT NOT NULL,
    planName TEXT,
    type TEXT NOT NULL,
    policyNumber TEXT NOT NULL,
    groupNumber TEXT,
    memberId TEXT,
    policyHolderName TEXT NOT NULL,
    expiryDate TEXT,
    website TEXT,
    customerServiceNumber TEXT,
    frontImagePath TEXT,
    backImagePath TEXT,
    colorTheme INTEGER,
    notes TEXT
);
